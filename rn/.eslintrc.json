{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prefer-stateless-function":0,
        "no-unused-vars":"warn",
        "react/prop-types": 0,
        "import/no-unresolved": 0,
        "global-require": 0
    },
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
    } 
}